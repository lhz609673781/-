#content .con-main .part-news>ul{
	margin-bottom: 40px;
}
#content .con-main .part-news>ul>li{
	float:left;
	width:10%;
	height:60px;
	background:#f59c00;
	text-align: center;
	font-size: 14px;
	color:#fff;
}
#content .con-main .part-news>ul>li:nth-of-type(1){
	background:#dc3e00;
}
#content .con-main .part-news>ul>li:nth-of-type(2){
	background:#f59c00;
}
#content .con-main .part-news>ul>li:nth-of-type(3){
	background:#80bb01;
}
#content .con-main .part-news>ul>li:nth-of-type(4){
	background:#3bbeb6;
}
#content .con-main .part-news>ul>li:nth-of-type(5){
	background:#358ff0;
}
#content .con-main .part-news>ul>li:nth-of-type(6){
	background:#9755b6;
}
#content .con-main .part-news>ul>li:nth-of-type(7){
	background:#1bb1dc;
}
#content .con-main .part-news>ul>li:nth-of-type(8){
	background:#30bf2d;
}
#content .con-main .part-news>ul>li:nth-of-type(9){
	background:#1b4fd2;
}
#content .con-main .part-news>ul>li+li{
	margin-left:1.25%;
}
#content .con-main .part-news>ul>li .data{
	display: block;
	height:30px;
	margin-top:4px;
	font-size: 24px;
	color:#fff;
}
#content .con-main .part-news h3{
	padding:15px 0;
	border:1px solid #e1e5ed;
	border-bottom: none;
	background:#f5f8fd;
	font-size: 18px;
	text-align: center;
	font-weight: normal;
}
#content .part-news h3>i{
	color:#ff6600;
	font-size: 24px;
}
#content .con-main .part-news .tab{
	width:50%;
	float:left;
}
#content .con-main .part-news table td{
	border:1px solid #e1e5ed;
	padding:10px 0;
	font-size:16px;
	line-height: 28px;
}
#content .con-main .part-news .tab-reverse td{
	border-left:1px solid transparent;
}
#content .part-news table tr .suc-icon,#content .part-news table tr .lose-icon{
	display: inline-block;
	vertical-align: middle;
	width:28px;
	height:28px;
	line-height: 28px;
	text-align: center;
	color:#fff;
	background:url(../images/icon.png) no-repeat;
	margin:0 1% 0 2%;
}
#content .part-news table tr:nth-of-type(1) .suc-icon{
	background-position: 0 -110px;
}
#content .part-news table tr:nth-of-type(2) .suc-icon{
	background-position: -36px -110px;
}
#content .part-news table tr:nth-of-type(3) .suc-icon{
	background-position: -71px -110px;
}
#content .part-news table tr:nth-of-type(4) .suc-icon{
	background-position: -109px -110px;
}
#content .part-news table tr:nth-of-type(5) .suc-icon{
	background-position: -143px -110px;
}
#content .part-news table tr .lose-icon{
	background-position: 0px -150px;
	font-size:12px;
	line-height: 17px;
}
#content .part-news>p{
	color:#636363;
	font-size: 12px;
	text-align: right;
	line-height:32px;
}
#content .part-collect table caption{
	font-weight: bold;
	font-size: 15px;
	color:#636363;
	padding:15px 0;
	border:1px solid #e1e5ed;
	border-bottom: none;
} 
#content .part-collect form .sel,#content .part-collect form label{
	font-size:14px;
	color:#5e6063;
	margin-right: 30px;
}
#content .part-collect form .sel>em{
	display: inline-block;
}
#content .part-collect form .sel>select{
	font-size: 14px;
    width: 82px;
}
#content .part-collect .btn{
	display: inline-block;
	margin-left:20px;
	margin-top:10px;
}
#content .part-collect .btn>input[type=text].kuang-yes{
	border: 1px solid #E1E5ED;
	display: inline-block;
	font-size: 14px;
	width:170px;
	color:#5E6063;
	border-radius: 5px;
}
#content .part-collect .btn input[type=button]{
	padding:3px 20px;
	line-height: 25px;
	margin-right:30px;
	border-radius: 5px;
	text-align: center;
	color:#fff;
	font-size: 14px;
	background: #358ff0;
	cursor: pointer;
}
#content .part-collect .btn input.export{
	text-indent:10px;
	background:#f59c00 url(../images/icon.png) no-repeat;
	background-position:-48px 9px ;
	padding:3px 10px 3px 20px;
}
#content .part-collect table{
	margin-top:10px;
	font-size: 13px;
}
#content .part-collect table th,#content .part-collect table td{
	border:1px solid #e1e5ed;
	padding:15px 0;
}
#content .part-collect table th{
	color:#358ff0;
	border-bottom: none;
	background:#f5f8fd;
	font-weight: normal;
}
#content .part-collect table td{
	text-align: center;
}
#content .part-collect>p{
	font-size: 12px;
	color:#8f8f8f;
	padding:10px 0;
}

/*��ҵ�������ͻ�ҳ����ʽ
#content .part-collect .depart .btn{
	margin-left:0;
}
#content .part-collect .depart .btn input[type=button]{
	margin-right:0;
}*/
#content .part-collect .btn input.import{
	text-indent:12px;
	background:#f59c00 url(../images/icon.png) no-repeat;
	background-position:-133px 9px ;
    padding: 3px 10px 3px 20px;
}
#content .part-collect .depart .mar-left{
	margin:0 10px 0 0;
}
#content .part-collect .collect-input{
	width:100px;
	height:22px;
	border:1px solid #e1e5ed;
	border-radius: 3px;
	text-indent: 4px;
}
#content .part-collect .depart label{
	margin-right:8px;
}

/*����ָ��*/
#content .part-collect .sale-target .mar-left{
	margin:0 30px 0 0;
}

@media (max-width: 1920px) {
	#content .con-main .part-news>ul{
		margin-bottom: 14px;
	}
	#content .con-main .part-news>ul>li{
		height: 46px;
		font-size: 12px;
	}
	#content .con-main .part-news>ul>li .data{
		font-size: 18px;
		height:22px;
		line-height: normal;
		margin-top:3px;
	}
	#content .con-main .part-news h3{
		padding:12px 0;
		font-size: 14px;
	}
	#content .part-news h3>span,#content .part-news h3>i,#content .part-news h3>em{
		font-size: 18px;
	}
	#content .con-main .part-news table td{
		font-size:14px;
		line-height: 28px;
	}
	#content .part-news table tr .lose-icon{
		width:22px;
		background:url(../images/icon-small.png) no-repeat;
		background-position: -83px -150px;
		font-size:12px;
		line-height: 21px;
	} 

	#content .part-collect form .sel,#content .part-collect form label{
		font-size:14px;
	}
	#content .part-collect form .sel>select{
		font-size: 14px;
		padding:0 4px;
		width:auto;
		border-radius: 3px;
	}
	#content .part-collect .btn input[type=button]{
		line-height: 21px; 
		font-size:14px;
	}
	#content .part-collect .btn input.export{
		background:#f59c00 url(../images/icon-small.png) no-repeat;
		background-position:-115px 7px ;
	}
	#content .part-collect table{
		font-size: 13px;
	}
	#content .part-collect table caption{
		font-weight: bold;
		font-size: 15px;
		padding:10px 0;
	}
	#content .part-collect table th,#content .part-collect table td{
		padding:12px 0;
	}
	#content .part-collect .btn input.import{
		background:#f59c00 url(../images/turn.png) no-repeat;
		background-position:9px 7px;
	}
