.order-info {
	width: 100%;
	background: #fff;
	border: 1px solid #ddd;
	border-radius: 3px;
}

.order-info .order-title {
	width: 100%;
	border-bottom: 1px solid #ddd;
	font-size: 12px;
	color: #4b68f2;
	line-height: 40px;
	padding-left: 10px;
}

.order-info .order-title span {
	color: #999;
	font-size: 12px;
	margin-left: 10px;
}

.order-info .order-content {
	padding: 10px 0;
}

.order-info .order-content .form-box {
	width: 100%;
	padding: 10px 0;
}

.order-info .order-content .form-box .form-control {
	min-width: 280px;
}

.order-info .order-content .form-box .form-control label {
	width: 110px;
	display: block;
	float: left;
	font-size: 12px;
	color: #333;
	line-height: 30px;
	text-align: right;
	padding-right: 10px;
}

.order-info .order-content .form-box .form-control input, select {
	float: right;
	width: 160px;
	height: 28px;
	border: 1px solid #ddd;
	border-radius: 3px;
	padding: 0 5px;
	font-size: 12px;
	color: #999;
}

.order-info .order-content table {
	width: 95%;
	margin: 0 auto;
	border-collapse: collapse;
}

.order-info .order-content table th, .order-info .order-content table td
	{
	border: 1px solid #ddd;
	height: 40px;
	text-align: center;
	font-size: 12px;
	color: #666;
	font-weight: 400;
}

.order-info .order-content table th a.delete, .order-info .order-content table td a.delete
	{
	width: 100px;
	height: 32px;
	border-radius: 3px;
	background: #FF5252;
	color: #fff;
	display: inline-block;
	line-height: 32px;
}

.order-info .order-content table th a.delete:hover, .order-info .order-content table td a.delete:hover
	{
	background: #C90A0A;
}

.order-info .order-content table th input, .order-info .order-content table td input
	{
	width: 100%;
	height: 100%;
	text-align: center;
}

.order-info .order-content table th {
	background: #eee;
	color: #333;
}

.order-info .order-content table tfoot tr td {
	text-align: left;
	padding-left: 10px;
}

.order-info .order-content table tfoot tr td a {
	color: #4fe2bf;
}

.order-info .order-content .btn-control {
	width: 100%;
	text-align: center;
}

.order-info .order-content .btn-control button {
	width: 120px;
	height: 36px;
	border-radius: 3px;
	color: #fff;
	font-size: 14px;
	margin: 20px 10px;
}

.order-info .order-content .btn-control .save {
	background: #4fe2bf;
}

.order-info .order-content .btn-control .reset {
	background: #ff6c00;
}

.form {
	width: 100%;
	height: 100%;
}

.address-width {
	width: 440px !important;
}

.liucheng {
	width: 100%;
	height: 280px;
	white-space: nowrap;
}

.liucheng .lc-wrap {
	overflow-y: hidden;
	overflow-x: scroll;
	width: 100%;
	height: 280px;
	font-size: 12px;
	line-height: 22px;
}
.liucheng .lc-wrap div{
	float:none !important;
	display:inline-block;
}
.liucheng .lc-wrap .load {
	position: relative;
	float: left;
	width: 140px;
	height: 260px;
	background: url("../images/load1.png") no-repeat center 42px;
}

.liucheng .lc-wrap .load .city {
	position: absolute;
	width: 140px;
	left: 0;
	text-align: center;
	top: 20px;
}

.liucheng .lc-wrap .load .address {
	width: 140px;
	position: absolute;
	left: 0;
	top: 123px;
	padding: 0 5px;
}

.liucheng .lc-wrap .load .time {
	color: #999;
}

.liucheng .lc-wrap .load-gray {
	background: url("../images/load0.png") no-repeat center 42px;
}

.liucheng .lc-wrap .car {
	float: left;
	width: 145px;
	height: 260px;
	background: url("../images/car1.png") no-repeat center 87px;
	position: relative;
}

.liucheng .lc-wrap .car .info {
	width: 145px;
	height: 80px;
	position: absolute;
	top: 172px;
	left: 0;
	padding: 5px 10px;
	line-height: 22px;
}

.liucheng .lc-wrap .car-gray {
	background: url("../images/car0.png") no-repeat center 87px !important;
}

.liucheng .lc-wrap .unload {
	float: left;
	width: 140px;
	position: relative;
	height: 260px;
	background: url("../images/unload1.png") no-repeat center 43px;
}

.liucheng .lc-wrap .unload .city {
	position: absolute;
	width: 140px;
	left: 0;
	text-align: center;
	top: 20px;
}

.liucheng .lc-wrap .unload .address {
	width: 140px;
	position: absolute;
	left: 0;
	top: 123px;
	padding: 0 5px;
}

.liucheng .lc-wrap .unload .time {
	color: #999;
}

.liucheng .lc-wrap .unload-gray {
	background: url("../images/unload0.png") no-repeat center 43px;
}

.add-lc {
	width: 100%;
	padding: 0 20px;
	text-align: right;
}

.add-lc button {
	background: url("../images/addBtn.png") center;
	width: 160px;
	height: 36px;
	cursor: pointer;
	outline: none;
}

.add-lc button:hover {
	opacity: 0.8;
}

#myModal .modal-dialog {
	margin-top: 150px;
	width: 600px;
	height: 330px;
}

#myModal .modal-dialog .modal-content {
	width: 600px;
	height: 330px;
}

#myModal .modal-dialog .modal-content form {
	width: 100%;
	height: 100%;
}

