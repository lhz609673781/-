html,body{
  height:100%;
  overflow: hidden;
}
.header{
  width:100%;
  height: 80px;
  background: url("../images/topNavBg.png") no-repeat center top;
  background-size: 100% 100%;
  h1{
    float: left;
    padding-left: 20px;
    width:50%;
    a{
      display: block;
      height: 80px;
      width:242px;
    }
    img{
      width:242px;
      height: 26px;
      margin:27px 0;
    }
  }
  .person-info{
    width:50%;
    height: 80px;
    padding:25px 0;
    text-align: right;
    padding-right: 20px;
    a{
      display: block;

      height: 100%;
    }
    .person-ico{
      display: inline-block;
      height:30px;
      width: 25px;
      background: url("../images/navUserIc.png") no-repeat center center;
    }
    span{
      line-height: 30px;
      display: inline-block;
      font-size: 12px;
      color: #ccc;
      height:30px;

    }
    .select-ico{
      width:25px;
      height: 30px;
      display: inline-block;
      background: url("../images/navArrowIc.png") no-repeat center center;
    }
  }
}

.main{
  width:100%;
  height: 100%;
  .nav{
    width:180px;
    background: #262a33;
    position: fixed;
    left:0;
    top:80px;
    .parent-ul{
      width:100%;
      .parent-li{
        width:100%;
        min-height:40px;
        font-size: 16px;
        line-height: 40px;
        color: #fff;
        a.parent-link{
          padding-left: 35px;
          width:100%;
          height:40px;
          color: #fff;
          .arrow{
            font-size: 18px;
            float: right;
            margin-right: 15px;
            font-style: normal;
            display: inline-block;
            text-rendering: auto;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
          }
          .arrow.plus:before{
            content: "+";
          }
          .arrow.reduce:before{
          }
        }

      }
      .active{
        a.parent-link{
          background-color: #4fe2bf!important;
        }
      }
      .active .arrow:before{
        content: "-"!important;
      }
      .nav1 .parent-link{
        background: url("../images/sideBarOrderIc.png") no-repeat 15px center;
      }
      .nav2 .parent-link{
        background: url("../images/sideBarCodeIc.png") no-repeat 15px center;

      }
    }
    .sub-ul{
      width:100%;
      display: none;
      li{
        width:100%;
        height:40px;
        a{
          line-height: 40px;
          color: #fff;
          font-size: 14px;
          padding-left: 30px;
          i{
            font-style: normal;
            font-size: 10px;
            padding:0 5px;
          }
        }
      }
    }
  }
  .main-content{
    width:100%;
    height:100%;
    background: #F3F3F3;
    padding-left:180px;
    .position{
      width:100%;
      height: 30px;
      background: #fff;
      border-bottom:1px solid #ddd;
      font:400 12px/30px "Microsoft Yahei";
      span{
        color: #999;
      }
      span.icon{
        padding:0 8px;
      }
      a{
        color: #486aff;
      }
    }
    .tab-hd{
      width:100%;
      height: 40px;
      padding:0 20px;
      border-bottom:1px solid #4768F3;
      ul{
        width:100%;
        height:100%;
        padding-top:6px;
        li{
          cursor: pointer;
          span{
            padding:0 5px;
          }
          float: left;
          margin-right: 10px;
          width:118px;
          height: 34px;
          text-align: center;
          font-size: 14px;
          color: #476bff;
          line-height: 32px;
          border:1px solid #4768F3;
          position: relative;
          border-top-left-radius: 3px;
          border-top-right-radius:3px;
          .close{
            position: absolute;
            top:2px;
            right:2px;
            font-size: 16px;
            line-height:16px;
            color: #fff;
          }
        }
        li.active,li:hover{
          background: #4768F3;
          color: #fff;
          .close{
            color: #fff;
          }
        }
      }
    }

    .J_mainContent{
      width:100%;
      padding:20px;
      .J_iframe{
      }
    }
  }
}