@CHARSET "UTF-8";
.SearchBtn{
	margin-left:15px;
}
hr{
	margin-top:10px;
	width:120%;
	margin-left:-10%;
	border:1px solid #8c8c8c;
}
.input_label{
	width: 165px;
}
.groupControlCar{
	width:350px;
	height:300px;
	margin-top: 10px;
	margin-bottom:20px;
	position: relative;
}
.AnchoredControlCar{
	height:250px;
	margin-top: 0px;
	margin-bottom: -33px;
}
.controlledCar{
	width: 199px;
    height: 60px;
    position: absolute;
    top: 45px;
    left: 78px;
    text-align: center;
    line-height: 60px;
    font-size: 17px;
    font-weight: 900;
    color: #fff;
    cursor: pointer;
    background: #f7a35c;
    box-shadow: 10px 10px 5px #999;
}
.Anchored-car{
	padding: 19px;
    background: #bce2f3;
    position: absolute;
    bottom: 68px;
    left: 14px;
    border: 1px solid #c2c2c2;
    border-radius: 74px;
    cursor: pointer;
    box-shadow: 10px 10px 5px #999;
}
.Self-support-car{
	right:0px;
	left:auto;
	background:#ededff;
}
.right-content{
	margin-left:40%;
	position:relative;
}
/*********轮播图样式************/ 
.right-container{
	position: relative;
}
.chartsSlider{
	width:80%;
	height:300px;
	/* margin: 0 auto !important; */
	overflow: hidden;
	top: 25px;
	margin-left: 10%;
}
.right-container .slider-container{
	height: 300px;
}
.right-container .slider{
	height:300px;
	text-align:center;
	padding-top:30px;
	box-sizing: border-box; 
}
.btn{
	width:50px;
	height: 50px;
	position:absolute;
	top:0;
	bottom:0;
	margin:auto;
	z-index:333333;
	cursor:pointer;
	display: none;
}
.prev-btn,.left-btn{
	left:48px;
	background: url(../images/prev.jpg) no-repeat;
	background-size: contain;
}
.next-btn,.right-btn{
	right:30px;
	background: url(../images/next.jpg) no-repeat;
	background-size: contain;
}
#dot-container{
	text-align: center;
	margin-top: 30px;
	margin-left:10%;
}
.dot{
	width: 10px;
    height: 10px;
    border-radius: 20px;
    display: inline-block;
    margin-left: 5px;
    cursor: pointer;
    border: 1px solid #8c8c8c;
}
.dot_active{
	background: #8c8c8c;
}
.barchrts-title,.slider-title{
	text-align: center;
    font-size: 20px;
    font-weight: 900;
    color: #666;
    margin-bottom: 10px;
}
.slider-title{
	margin-top: 15px;
	margin-bottom: 0px;
}
/******************************************************************************************************/
.menuList{
	top: 37px;
} 
.Table-rendering{
	cursor: pointer;
}
.bar-content{
	width: 100%;
	display: block;
	height: 350px;
	position: relative;
}

/* 设置容器宽高 */
.swiper-container {
    width: 100%;
    height: 90%;
}
.swiper-slide {
    text-align: center;
    font-size: 18px;
    background: #fff;
    /* Center slide text vertically */
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.swiper-container-vertical>.swiper-pagination-bullets{
	right: 0px !important;
}
.swiper-container-vertical>.swiper-wrapper{
	left: -8px !important;
}
.pie-draw{
	display: inline-block;
	width: 90%;
	height: 330px;
	position: relative;
	margin-left: 5%;
	overflow: hidden;
}
.pieDrawTitle{
	width: 20px;
    font-size: 20px;
    font-weight: 900;
    color: #666;
    position: relative;
    top: -153px;
    display: inline-block;
    margin-left: 5%;
}
#branchOfficeName{
	position: absolute;
    bottom: 25%;
    left: 10%;
    z-index: 999;
    font-weight: 900;
    width: 20px;
    font-size: 12px;
}
.pie-content{
	width: 80%;
	height: 400px;
	display: inline-block;
	margin-left: 10%;
}
.menushow:hover{
	text-decoration: underline;
	color: #000;
	corsur: pointer;
}
.Anchored-container{
	display: none;
	position: relative;
	width: 80%;
	margin-left: 10%;
}
.left-container{
	float:left;
	width: 40%;
}
.branchOfficeRanking,.CarrierRanking{
    position: relative;
    margin-top: 24px;
    height: 450px;
}
.Carrier-chart,.chartsShow,
.branchOfficeRanking .slider-container, .branchOfficeRanking .slider,
.CarrierRanking .slider-container, .CarrierRanking .slider{
	height: 450px;
}
.ranking-title{
	text-align: center;
    font-size: 20px;
    font-weight: 900;
    position: relative;
    top: 41px;
    color: #666;
}
.chartsShow,.Carrier-chart{
	width: 100%;
    margin: 0 auto;
    overflow: hidden;
}
.branchOfficeRanking .right-btn,
.CarrierRanking .next-btn{
	right: -52px !important;
}
.branchOfficeRanking .left-btn,
.CarrierRanking .prev-btn{
	left: 0px !important;
}
.selected-li{
	background: #1E9FFF;
	color: #fff !important;
}
.swiper-pagination-bullet-active {
    background: #f7a35c !important;
}
.controCar-title {
    width: 14px;
    position: absolute;
    z-index: 999;
    padding: 109px 10px;
    font-size: 12px;
    line-height: 13px;
    background: #fff;
}
@media screen and (min-width:1500px){
	.groupControlCar {
	    margin-left: 100px;
	}
}
@media screen and (max-width:1570px){
	.right-container .next-btn{
		right:15px;
	}
}

@media screen and (max-width:1190px){
	.right-container .next-btn{
		right:0px;
	}
	.right-container .btn{
		width:23px;
	}
	.pie-content{
		margin-left: 0%;
	}
}