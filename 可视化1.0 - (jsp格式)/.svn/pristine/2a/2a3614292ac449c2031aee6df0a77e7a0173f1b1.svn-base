label.rCheckbox {width:18px;cursor:pointer;background: url(../../images/checkbox.png) no-repeat 0 0;margin-bottom:0; line-height:14px; height:14px; display:inline-block; } 
label.checked {background-position:0 bottom;  } 
label.disabled{background-position:left 0px;}

