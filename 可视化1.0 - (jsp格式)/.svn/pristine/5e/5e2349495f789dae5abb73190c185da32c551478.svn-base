/* 我的页面样式 */
.aboutmine .img,.bindphone .img-phone{
    width:2.67rem;
    height:2.67rem;
    border:1px solid #ff5a01;
    margin: 0.8rem auto 0.53rem;
    border-radius: 50%;
    background:#fff url(../img/icon_p.png) no-repeat center;
}
.aboutmine .link{
    margin-top:2.67rem;
}
.aboutmine .link>a{
    display: block;
    width:80%;
    height:1.07rem;
    line-height: 1.07rem;
    text-align: center;
    margin:0.4rem auto;
    border:1px solid #ff5a01;
    background:#fff;
    border-radius: 0.11rem;
    color:#ff5a01;
    text-decoration: none;
    font-size: 0.4rem;
}
.aboutmine .link>a:active{
    background:#ff5a01;
    color:#fff;
}

/* 绑定号码样式 */
.bindphone .img-phone{
    margin: 0.8rem auto 1.2rem;
}
.regsin .reg-list{
    width:80%;
    position: relative;
    margin:0 auto 0.4rem;
}
.regsin .reg-list>input{
    width:100%;
    padding:0.27rem 0;
    font-size: 0.4rem;
    border-radius: 0.11rem;
    border:1px solid #c6c6c6;
    text-indent:1.73rem;
}
.regsin .reg-list>label{
    position: absolute;
    top:0;
    left:20px;
    font-size: 0.4rem;
    height:1.07rem;
    line-height: 1.07rem;
}
.regsin .reg-list>input.input-code{
    width:60%;
    float:left;
}
.regsin .reg-list .code{
    float:right;
    width:35%;
    height:1.04rem;
    line-height: 1.04rem;
    border:1px solid #ff5a01;
    font-size: 0.35rem;
    color:#ff5a01;
    background: #fff;
    text-align: center;
    border-radius: 0.05rem;
}
.regsin .btn{
    display: block;
    width:80%;
    height: 1.07rem;
    font-size: 0.4rem;
    color:#fff;
    background: #ff5a01;
    margin:0.93rem auto 0;
    border-radius: 0.11rem;
}

/* 我的信息页面样式 */
.my-message .message1{
    margin:0.27rem 0;
    border-top:1px solid #dfdee3;
    border-bottom:1px solid #dfdee3;
}
.my-message .message1>input{
    width:100%;
    height:1.07rem;
    text-indent: 0.53rem;
    margin-bottom: 0.27rem;
}
.my-message .message2{
    background: #fff;
}
.my-message .message2>h3{
    font-weight: normal;
    color:#333;
    font-size: 0.45rem;
    padding:0.27rem 0 0.27rem 0.27rem;
}
.my-message .message2 .part{
    border-top:1px solid #dfdee3;
    padding:0.27rem 0 0.27rem 0.27rem;
}
.my-message .message2 .part-left{
  	text-align: center;
    color:#908f9d;
    margin-bottom: 0.4rem;
}
.my-message .message2 .part-right .z_file{
	width:1.6rem;
	height:1.6rem;
	float:left;
	position: relative;
}
.my-message .message2 .part-right .z_file .add-img{
	width:100%;
	height:100%;
}
.my-message .message2 .part-right .z_file>input[type=file]{
	width:1.6rem;
	height:1.6rem;
	position: absolute;
	top:0;
	left:0;
	z-index:10;
	opacity: 0;
    filter: alpha(opacity:0);	
}
.my-message .message2 .part-left>span{
    display: block;
    color:#5e5e76;
    font-size: 0.37rem;
    padding:0.24rem 0;
}
.my-message .message3>input[type=button]{
    display: block;
    width:80%;
    height: 1.07rem;
    font-size: 0.4rem;
    color:#fff;
    background: #ff5a01;
    border-radius: 0.11rem;
    margin:0.53rem auto;
}


