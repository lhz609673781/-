/*分页插件样式*/
.pageTest{width:500px;margin:50px auto 20px;}
.activP{
  background-color: #367fa9!important;
  color: #fff!important;
  border:1px solid #367fa9;
}
.pagingUl{
  float:right;
  padding:0;
  margin:0;
  list-style: none;
}
.pagingUl li{
  float: left;
  list-style: none;
  text-align: center;
  margin-left:-1px;
}
.pagingUl li a{
  text-decoration: none;
  display: block;
  text-align: center;
  border: 1px solid #ddd;
  width:30px;
  height:30px;
  line-height:30px;
  padding: 0;
  background: #fafafa;
  color: #666;
  font-size: 12px;
  
}
.prv, .next {
  padding:0 10px;
  height: 30px;
  float: left;
  text-align: center;
  line-height: 30px;
  border: 1px solid #ddd;
  cursor: pointer;
  font-size:12px;
}
.first, .last {
  padding:0 10px;
  height: 30px;
  font-size: 12px;
  text-align: center;
  line-height: 30px;
  border: 1px solid #ddd;
  cursor: pointer;
}
.next,.last,.first{margin-left:-1px;}
.pagingUl li:hover>a{background:#367fa9;color:#fff;}
.next:hover,.last:hover,.first:hover,.prv:hover{background:#367fa9;color:#fff;}
